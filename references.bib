@article{nielsen2007,
  title = {The {{Regularized Iteratively Reweighted MAD Method}} for {{Change Detection}} in {{Multi-}} and {{Hyperspectral Data}}},
  author = {Nielsen, Allan Aasbjerg},
  date = {2007},
  journaltitle = {IEEE Transactions on Image Processing},
  volume = {16},
