@article{almeida2021,
  title = {Study of the {{Urban Heat Island}} ({{UHI}}) {{Using Remote Sensing Data}}/{{Techniques}}: {{A Systematic Review}}},
  shorttitle = {Study of the {{Urban Heat Island}} ({{UHI}}) {{Using Remote Sensing Data}}/{{Techniques}}},
  author = {family=Almeida, given=Cátia Rodrigues, prefix=de, useprefix=false and Teodoro, Ana Cláudia and Gonçalves, Artur},
  date = {2021-10},
  journaltitle = {Environments},
